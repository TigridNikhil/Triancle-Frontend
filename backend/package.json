{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "set NODE_ENV=development&& nodemon index.js",
    "start:prod": "set NODE_ENV=production&& nodemon index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo",
    "dev:seed": "cross-env NODE_ENV=development npm run db:seed",
    "dev:seed:undo": "cross-env NODE_ENV=development npm run db:seed:undo",
    "dev:migrate": "cross-env NODE_ENV=development npm run db:migrate",
    "dev:migrate:undo": "cross-env NODE_ENV=development npm run db:migrate:undo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "config": "^3.3.11",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "cross-env": "^7.0.3",
    "csv-writer": "^1.6.0",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "html-pdf": "^1.5.0",
    "http-errors": "~1.6.3",
    "jade": "^1.9.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.0",
    "otp-generator": "^4.0.1",
    "pdfkit": "^0.15.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.5",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.0.3",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.1"
  }
}
